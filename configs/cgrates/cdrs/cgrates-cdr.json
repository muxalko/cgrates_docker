	},

	"data_exports": {
		"export_format":"*http_json_cdr",
		"export_path":"http://10.224.228.188/processor/api/processCdr",
		"filters" :["*string:~*req.RunID:*default","*string:~*req.ToR:*data"],
	},

	"sms_exports": {
		"export_format":"*http_json_cdr",
